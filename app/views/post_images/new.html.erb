<%= render 'layouts/errors', obj: @new %>

<h1>画像投稿フォーム</h1>
<%= form_with model: @new, local: true do |f| %>
  <label>画像</label><br>
  <%= f.attachment_field :image %><br>
  <label>ショップ名</label><br>
  <%= f.text_field :shop_name %><br>
  <label>説明</label><br>
  <%= f.text_area :caption %><br>
  <%= f.submit "投稿" %>
<% end %>